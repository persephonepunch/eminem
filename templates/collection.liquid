{% layout 'collection' %}
      
  <div class="preloader"><img src="{{ 'EminemLogoBL.svg' | asset_url }}" width="150" height="70" alt="" class="logo-preloader">
    <div data-w-id="bce50fe6-f179-2dd3-283e-6c552986c2d6" data-animation-type="lottie" data-src="https://uploads-ssl.webflow.com/5e3a95568c461017d7e7a30b/5e3be132807b8721620cf527_lf30_editor_n6xHam.json" data-loop="0" data-direction="1" data-autoplay="1" data-is-ix2-target="0" data-renderer="svg" data-default-duration="2.9696361820138772" data-duration="0" class="lottie-animation-2"></div>
  </div>
  <div style="display:none" class="main">
    <div data-collapse="medium" data-animation="default" data-duration="400" data-no-scroll="1" role="banner" class="navbar1 active-nav1 white w-nav">
      <div class="nav1-wrap">
        <a href="/pages/psd-home" class="w-inline-block">
          <div class="div-block-326"><img src="{{ 'EminemLogo.png' | asset_url }}" alt=""></div>
        </a>
        <div class="nav1-menu-wrap">{% if customer %}<a href="/account" class="main-button w-button">{{ 'collection.A_0' | t }}</a>{% endif %}
          <div data-node-type="commerce-cart-wrapper" data-open-product="" data-wf-cart-type="rightSidebar" data-wf-cart-query="" data-wf-page-link-href-prefix="" class="w-commerce-commercecartwrapper cart"><a href="#" data-node-type="commerce-cart-open-link" class="w-commerce-commercecartopenlink cart-button w-inline-block"><img src="{{ '5ea444450e34982891325943_bag.svg' | asset_url }}" width="20" alt=""><div class="w-commerce-commercecartopenlinkcount cart-quantity" udy-el="cart-count">{{ cart.item_count }}</div></a>
            <div data-node-type="commerce-cart-container-wrapper" style="display:none" class="w-commerce-commercecartcontainerwrapper w-commerce-commercecartcontainerwrapper--cartType-rightSidebar cart-wrap">
              <div data-node-type="commerce-cart-container" class="w-commerce-commercecartcontainer cart-container">
                <div class="w-commerce-commercecartheader cart-header"><img src="{{ 'EminemLogoBL.svg' | asset_url }}" width="83" alt="">
                  <h4 class="w-commerce-commercecartheading cart-heading">{{ 'collection.H4_1' | t }}</h4><a href="#" data-node-type="commerce-cart-close-link" class="w-commerce-commercecartcloselink close-button w-inline-block"><img src="{{ 'CloseCart2x_1CloseCart2x.png' | asset_url }}" width="11" alt="" class="cart-close-icon"></a></div>
                <div class="w-commerce-commercecartformwrapper">
                  <form data-node-type="commerce-cart-form" class="w-commerce-commercecartform default-state-2 {% if cart.item_count == 0 %}udesly-display-none{% endif %}">
                    
                    
                    <ul role="list" class="mini-cart-list" udy-el="items-list">
                      {% for item in cart.items %}<li class="cart-item-2">
                        <div class="mini-cart-row">
                          <div class="variations-main-wrapper">
                            <div class="mini-cart-featured-image" style="background-image: url({{ item | img_url: 'large' }})"></div>
                            <div class="minicart-info">
                              <div class="text-block-38">{{ item.product.title }}</div>
                              <div class="text-block-39">{{ item.price | money }}</div><a href="/cart/change?line={{ forloop.index }}&amp;quantity=0" class="link-9" udy-el="remove-item">{{ 'collection.A_2' | t }}</a></div>
                          </div>
                          <div class="cart-quantity-3">{{ item.quantity }}</div>
                        </div>
                      </li>{% endfor %}
                    </ul>
                    <div class="w-commerce-commercecartfooter cart-footer {% if cart.item_count == 0 %}udesly-display-none{% endif %}" udy-el="cart-actions">
                      <div class="w-commerce-commercecartlineitem cart-line-item">
                        <div class="size5-text">{{ 'collection.DIV_3' | t }}</div>
                        <div class="w-commerce-commercecartordervalue size6-text" udy-el="cart-total">{{ cart.total_price | money }}</div>
                      </div>
                      <div>
                        <div data-node-type="commerce-cart-quick-checkout-actions" class="web-payments-2">
                          <a data-node-type="commerce-cart-apple-pay-button" style="background-image:-webkit-named-image(apple-pay-logo-white);background-size:100% 50%;background-position:50% 50%;background-repeat:no-repeat" class="w-commerce-commercecartapplepaybutton web-payment-button">
                            <div></div>
                          </a><a data-node-type="commerce-cart-quick-checkout-button" style="display:none" class="w-commerce-commercecartquickcheckoutbutton"><svg class="w-commerce-commercequickcheckoutgoogleicon" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="16" height="16" viewBox="0 0 16 16"><defs><polygon id="google-mark-a" points="0 .329 3.494 .329 3.494 7.649 0 7.649"></polygon><polygon id="google-mark-c" points=".894 0 13.169 0 13.169 6.443 .894 6.443"></polygon></defs><g fill="none" fill-rule="evenodd"><path fill="#4285F4" d="M10.5967,12.0469 L10.5967,14.0649 L13.1167,14.0649 C14.6047,12.6759 15.4577,10.6209 15.4577,8.1779 C15.4577,7.6339 15.4137,7.0889 15.3257,6.5559 L7.8887,6.5559 L7.8887,9.6329 L12.1507,9.6329 C11.9767,10.6119 11.4147,11.4899 10.5967,12.0469"></path><path fill="#34A853" d="M7.8887,16 C10.0137,16 11.8107,15.289 13.1147,14.067 C13.1147,14.066 13.1157,14.065 13.1167,14.064 L10.5967,12.047 C10.5877,12.053 10.5807,12.061 10.5727,12.067 C9.8607,12.556 8.9507,12.833 7.8887,12.833 C5.8577,12.833 4.1387,11.457 3.4937,9.605 L0.8747,9.605 L0.8747,11.648 C2.2197,14.319 4.9287,16 7.8887,16"></path><g transform="translate(0 4)"><mask id="google-mark-b" fill="#fff"><use xlink:href="#google-mark-a"></use></mask><path fill="#FBBC04" d="M3.4639,5.5337 C3.1369,4.5477 3.1359,3.4727 3.4609,2.4757 L3.4639,2.4777 C3.4679,2.4657 3.4749,2.4547 3.4789,2.4427 L3.4939,0.3287 L0.8939,0.3287 C0.8799,0.3577 0.8599,0.3827 0.8459,0.4117 C-0.2821,2.6667 -0.2821,5.3337 0.8459,7.5887 L0.8459,7.5997 C0.8549,7.6167 0.8659,7.6317 0.8749,7.6487 L3.4939,5.6057 C3.4849,5.5807 3.4729,5.5587 3.4639,5.5337" mask="url(#google-mark-b)"></path></g><mask id="google-mark-d" fill="#fff"><use xlink:href="#google-mark-c"></use></mask><path fill="#EA4335" d="M0.894,4.3291 L3.478,6.4431 C4.113,4.5611 5.843,3.1671 7.889,3.1671 C9.018,3.1451 10.102,3.5781 10.912,4.3671 L13.169,2.0781 C11.733,0.7231 9.85,-0.0219 7.889,0.0001 C4.941,0.0001 2.245,1.6791 0.894,4.3291" mask="url(#google-mark-d)"></path></g></svg><svg class="w-commerce-commercequickcheckoutmicrosofticon" xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16"><g fill="none" fill-rule="evenodd"><polygon fill="#F05022" points="7 7 1 7 1 1 7 1"></polygon><polygon fill="#7DB902" points="15 7 9 7 9 1 15 1"></polygon><polygon fill="#00A4EE" points="7 15 1 15 1 9 7 9"></polygon><polygon fill="#FFB700" points="15 15 9 15 9 9 15 9"></polygon></g></svg><div>{{ 'collection.DIV_4' | t }}</div></a></div>
                        <div style="position:relative" data-wf-paypal-button="{&quot;layout&quot;:&quot;horizontal&quot;,&quot;color&quot;:&quot;black&quot;,&quot;shape&quot;:&quot;rect&quot;,&quot;label&quot;:&quot;paypal&quot;,&quot;tagline&quot;:false}" class="paypal">
                          <div style="position:absolute;top:0;left:0;width:100%;height:100%;opacity:0;z-index:999;cursor:auto"></div>
                        </div><a href="/checkout" value="Continue to Checkout" data-node-type="cart-checkout-button" class="w-commerce-commercecartcheckoutbutton main-button" data-loading-text="Hang Tight...">{{ 'collection.A_5' | t }}</a></div>
                    </div>
                  </form>
                  <div class="w-commerce-commercecartemptystate empty-state dark-cart-empty-state {% if cart.item_count != 0 %}udesly-display-none{% endif %}" udy-el="no-items-in-cart">
                    <div>{{ 'collection.DIV_6' | t }}</div>
                  </div>
                  <div style="display:none" data-node-type="commerce-cart-error" class="w-commerce-commercecarterrorstate error-message">
                    <div class="w-cart-error-msg" data-w-cart-quantity-error="Product is not available in this quantity." data-w-cart-checkout-error="Checkout is disabled on this site." data-w-cart-general-error="Something went wrong when adding this item to the cart." data-w-cart-cart_order_min-error="Cart failed.">{{ 'collection.DIV_7' | t }}</div>
                  </div>
                </div>
              </div>
            </div>
          <script id="mini-cart-template">
          window.udeslyMiniCartTemplate = function(item) {
            return `<li class="cart-item-2" data-item-id="${item.id}">
                        <div class="mini-cart-row">
                          <div class="variations-main-wrapper">
                            <div class="mini-cart-featured-image" style="background-image: url(${item.image})"></div>
                            <div class="minicart-info">
                              <div class="text-block-38">${item.title}</div>
                              <div class="text-block-39">${item.price}</div><a href="${item.remove}" class="link-9" udy-el="remove-item">remove</a></div>
                          </div>
                          <div class="cart-quantity-3">${item.quantity}</div>
                        </div>
                      </li>`
          }
          </script></div>
          <div class="menu-button w-nav-button"><img src="{{ 'menu-black.svg' | asset_url }}" width="15" alt="" class="menu-icon"></div>
        </div>
      </div>
    </div>
    <div class="category-hero">
      <div class="div-block-303">
        <h1 class="category-name">{{ collection.title }}</h1>
      </div>
      <form action="/search" post-type="product" class="search-products w-form" method="get" role="search"><input type="search" class="search-input w-input" maxlength="256" name="q" placeholder="Search a product…" id="search" required="" value="{{ search.terms | escape }}"><input type="submit" value="Search" class="main-button search-box w-button"></form>
    </div>
    <div class="section-20">
      {% paginate collection.products by settings.collection_products_limit %}<div class="collection-list-wrapper-2 w-dyn-list">
        <div role="list" class="collection-list-2 w-dyn-items">
          {% for product in collection.products %}<div role="listitem" class="w-dyn-item">
            <div class="collection-item__card-product"><a href="{{ product.url | within: collection.url }}" class="card-product_wrapper-image w-inline-block"><img src="{{ product.featured_image.src | img_url: 'large' }}" alt="{{ product.featured_image.alt }}" class="wrapper-image__img">{% if product.compare_at_price > product.price %}<div class="on-sale-badge-card"><div class="text-block-122">{{ 'collection.DIV_8' | t }}</div></div>{% endif %}</a>
              <div class="card-blog__wrapper-text">
                <div class="wrapper-text__line-2"></div>
                <div class="link__text-2 blog">{% if product.price_varies %}{{ product.price_min | money }} - {{ product.price_max | money }}{% else %}{{ product.price | money }}{% endif %}</div>
                <h1 class="wrapper--text__title">{{ product.title }}</h1>
                <div udy-el="loop-add-to-cart">
                  <form data-node-type="commerce-add-to-cart-form" class="w-commerce-commerceaddtocartform" action="/cart/add"><input type="submit" value="Add To Cart" data-loading-text="Adding to cart..." class="w-commerce-commerceaddtocartbutton submit-button"><input type="hidden" name="id" value="{{ product.variants.first.id }}"></form>
                  <div style="display:none" class="w-commerce-commerceaddtocartoutofstock">
                    <div>{{ 'collection.DIV_9' | t }}</div>
                  </div>
                  <div data-node-type="commerce-add-to-cart-error" style="display:none" class="w-commerce-commerceaddtocarterror" udy-el="add-to-cart-errors">
                    <div class="w-add-to-cart-error-msg" data-w-add-to-cart-quantity-error="Product is not available in this quantity." data-w-add-to-cart-general-error="Something went wrong when adding this item to the cart." data-w-add-to-cart-buy-now-error="Something went wrong when trying to purchase this item." data-w-add-to-cart-checkout-disabled-error="Checkout is disabled on this site.">{{ 'collection.DIV_10' | t }}</div>
                  </div>
                </div>
              </div>
            </div>
          </div>{% endfor %}
        </div>
        {% if collection.products == empty %}<div class="w-dyn-empty">
          <div>{{ 'collection.DIV_11' | t }}</div>
        </div>{% endif %}
        <div class="w-pagination-wrapper pagination-2">{% if paginate.previous.is_link %}<a href="{{ paginate.previous.url }}" class="w-pagination-previous main-button"><svg class="w-pagination-previous-icon" height="12px" width="12px" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 12 12" transform="translate(0, 1)"><path fill="none" stroke="currentColor" fill-rule="evenodd" d="M8 10L4 6l4-4"></path></svg><div class="w-inline-block">{{ 'collection.DIV_12' | t }}</div></a>{% endif %}
          {% if paginate.next.is_link %}<a href="{{ paginate.next.url }}" class="w-pagination-next main-button">
            <div class="w-inline-block">{{ 'collection.DIV_13' | t }}</div><svg class="w-pagination-next-icon" height="12px" width="12px" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 12 12" transform="translate(0, 1)"><path fill="none" stroke="currentColor" fill-rule="evenodd" d="M4 2l4 4-4 4"></path></svg></a>{% endif %}
        </div>
      </div>{% endpaginate %}
    </div>
    <div class="footer-wrap">
      <div class="newsletter-wrapper">
        <div class="subscribe-flex">
          <div>
            <p class="subscribe-text">{{ 'collection.P_14' | t }}</p>
          </div>
          <div>
            <div class="form-block-2 w-form">{% form 'customer' %}
              <div udesly-add-class="w-clearfix"></div><input type="email" class="text-field-17 w-input" maxlength="256" name="contact[email]" placeholder="Sign up now" id="email-2" required=""><input type="submit" value="Sign Up" data-wait="Please wait..." class="submit-button-3 w-button">
              
              
            </div><div class="form-block-2 w-form">{% if form.errors %}<div class="w-form-fail" style="display: inherit;">
                <div>{% include 'form-errors' %}</div>
              </div>{% endif %}{% if form.posted_successfully? %}<div class="w-form-done" style="display: inherit;">
                <div>{{ 'collection.DIV_15' | t }}</div>
              </div>{% endif %}</div>{% endform %}
          </div>
        </div>
      </div>
      <div class="w-layout-grid grid"><a href="/pages/old-home" id="w-node-2152929aeb40-929aeb3e" class="footer-logo-wrapper w-inline-block"><img src="{{ 'EminemLogoBL.svg' | asset_url }}" width="100" alt=""></a>
        <h5 id="w-node-2152929aeb42-929aeb3e" class="footer-heading">{{ 'collection.H5_16' | t }}</h5>
        <h5 id="w-node-2152929aeb46-929aeb3e" class="footer-heading">{{ 'collection.H5_17' | t }}</h5><a id="w-node-2152929aeb56-929aeb3e" href="#" class="footer-link">{{ 'collection.A_18' | t }}</a><a id="w-node-2152929aeb58-929aeb3e" href="#" class="footer-link">{{ 'collection.A_19' | t }}</a><a id="w-node-2152929aeb64-929aeb3e" href="#" class="footer-link">{{ 'collection.A_20' | t }}</a><a id="w-node-2152929aeb66-929aeb3e" href="https://www.facebook.com/UMPG" class="footer-link">{{ 'collection.A_21' | t }}</a><a id="w-node-2152929aeb68-929aeb3e" href="https://www.instagram.com/umpg/" class="footer-link">{{ 'collection.A_22' | t }}</a><a id="w-node-2152929aeb6a-929aeb3e" href="https://twitter.com/umpg" class="footer-link">{{ 'collection.A_23' | t }}</a>
        <div id="w-node-2152929aeb6c-929aeb3e" class="copyright-wrap">
          <div class="copyright">{{ 'collection.DIV_24' | t }}<a href="https://www.eclipse.srl/" target="_blank" class="copyright-link-small"></a></div>
        </div>
      </div>
    </div>
    <div data-w-id="dc855bcd-7523-d583-8df1-cb2e9f6c7f30" class="tag">
      <div class="product-wrap">
        <div class="left-wrap">
          <div class="con-wrap-2"><img src="{{ 'ewhite.svg' | asset_url }}" width="25" alt="" class="banner-icon"><a href="https://www.umusicpub.com/us/Artists/E/Eminem.aspx" target="_blank" class="banner-home-link w-inline-block"></a></div>
          <div class="banner-title-wrap">
            <div class="banner-title-2">{{ 'collection.DIV_25' | t }}<br></div>
            <div class="banner-author-wrap"><a href="https://flowbase.co" target="_blank" class="banner-author">{{ 'collection.A_26' | t }}<span class="flowbase-underline"></span></a></div>
          </div>
        </div><a href="https://www.universal-music.de/eminem/videos/lose-yourself-88491" target="_blank" class="banner-clone-button-2 w-button">{{ 'collection.A_27' | t }}</a>
        <div data-w-id="dc855bcd-7523-d583-8df1-cb2e9f6c7f40" class="banner-close-card"><img src="https://uploads-ssl.webflow.com/5aed5f7ff167d632686865b0/5beac8ffed94f5c015f87bb8_x.svg" width="15" height="15" alt="" class="banner-exit"></div>
      </div>
    </div>
  </div>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.4.1.min.220afd743d.js?site=5eed6d30cb5030b8331a4ef5" type="text/javascript" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
  <script src="{{ 'sampleshopifytemplate-783f9efad63d29184.js' | asset_url }}" type="text/javascript"></script>
